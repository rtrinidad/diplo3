<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="AsoEJB">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
      	<jta-data-source>java:jboss/datasources/AsoEJBDS</jta-data-source>
      	<class>py.edu.ucsa.aso.ejb.entities.CodigoSeguridad</class>
      	<class>py.edu.ucsa.aso.ejb.entities.Dominio</class>
      	<class>py.edu.ucsa.aso.ejb.entities.EstadosSocio</class>
      	<class>py.edu.ucsa.aso.ejb.entities.Exposicion</class>
      	<class>py.edu.ucsa.aso.ejb.entities.MensajeEnviadoSocio</class>
      	<class>py.edu.ucsa.aso.ejb.entities.MontoCuota</class>
      	<class>py.edu.ucsa.aso.ejb.entities.Opcion</class>
      	<class>py.edu.ucsa.aso.ejb.entities.PagoCuotaSocio</class>
      	<class>py.edu.ucsa.aso.ejb.entities.ParticExpoSocio</class>
      	<class>py.edu.ucsa.aso.ejb.entities.Rol</class>
      	<class>py.edu.ucsa.aso.ejb.entities.Socio</class>
      	<class>py.edu.ucsa.aso.ejb.entities.TematicaParticExpoSocio</class>
      	<class>py.edu.ucsa.aso.ejb.entities.Usuario</class>
		<properties>
         <property name="hibernate.dialect" 
                  value="org.hibernate.dialect.PostgreSQLDialect"/>
         <property name="hibernate.hbm2ddl.auto" value="update"/>
         <property name="hibernate.show_sql" value="true" />
      </properties>
      
	</persistence-unit>
</persistence>


